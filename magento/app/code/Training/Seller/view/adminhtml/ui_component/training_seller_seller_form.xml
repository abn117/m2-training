<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">training_seller_seller_form.seller_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Training\Seller\Block\Adminhtml\Seller\Edit\BackButton" name="back"/>
			<button class="Training\Seller\Block\Adminhtml\Seller\Edit\DeleteButton" name="delete"/>
			<button class="Training\Seller\Block\Adminhtml\Seller\Edit\SaveButton" name="save"/>
			<button class="Training\Seller\Block\Adminhtml\Seller\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>training_seller_seller_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>training_seller_seller_form.seller_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="seller_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Training\Seller\Model\Seller\DataProvider" name="seller_form_data_source">
			<settings>
				<requestFieldName>seller_id</requestFieldName>
				<primaryFieldName>seller_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label>General</label>
		</settings>
		<field formElement="input" name="seller_id" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Seller</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">seller_id</label>
				<dataScope>seller_id</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="identifier" sortOrder="20">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Seller</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">identifier</label>
				<dataScope>identifier</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="input" name="name" sortOrder="30">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Seller</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="created_at" sortOrder="40">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Seller</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">created_at</label>
				<dataScope>created_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
		<field formElement="date" name="updated_at" sortOrder="50">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">Seller</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">updated_at</label>
				<dataScope>updated_at</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
